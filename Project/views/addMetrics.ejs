<div id="form">
  <p>Records Metrics</p>
  <hr />
  <form action="/add" method="post">
    <div class="form-group">
      <label for="form_timestamp">timestamp</label>
      <input id="form_timestamp" type="number" min="0" name="timestamp" />
    </div>
    <div class="form-group">
      <label for="form_value">value</label>
      <input id="form_value" type="number" min="0" name="value" />
    </div>
    <button class="btn btn-primary btn-block" type="submit" value="Submit">Add</button>
  </form>
  <hr />
  <button class="btn btn-success btn-block" type="submit" href="/" onClick='document.location.href="/"'>
    return
  </button>
</div>