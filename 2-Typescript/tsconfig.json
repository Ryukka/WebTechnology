{
  "compileOnSave": true,
  "compilerOptions": {
    "noImplicitAny": false,
    "target": "es5",                          
    "module": "commonjs",                     
    "strict": true,                           
    "esModuleInterop": true,                  
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "moduleResolution": "node",
  },
"include": [
    "src/*"
  ],
"exclude": [
    "node_modules",
    "**/*.spec.ts"
  ]
}
